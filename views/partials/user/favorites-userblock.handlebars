<div class="card" id="note{{@index}}">
  <div class="card-header">
    <span><h3>{{id}}. {{poem_title}} by: {{poem_author}}</h3><a href="" class="deleteFav" data-id="{{id}}">X</a></span>
  </div>
  <div class="card-body">
    <blockquote class="blockquote mb-{{@index}}">
      <p>Favorited on: {{createdAt}}</p>
      <a class="btn btn-primary" href="/poems/{{poem_title}}/{{poem_author}}" role="button">Poem Page</a>
    </blockquote>
  </div>
</div>